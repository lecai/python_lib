  {
    "module": "base",
    "app": "nebula",
    "name": "HTTP_CLICK",
    "remark": "点击",
    "visible_name": "点击",
    "status": "enable",
    "type": "filter",
    "source": [
      {
        "app": "nebula",
        "name": "HTTP_DYNAMIC"
      }
    ],
    "filter": {
      "type": "or",
      "condition": [
        {
          "object": "method",
          "operation": "==",
          "value": "POST",
          "type": "simple"
        },
        {
          "type": "and",
          "condition": [
            {
              "object": "method",
              "operation": "==",
              "value": "GET",
              "type": "simple"
            },
            {
              "object": "s_type",
              "operation": "contains",
              "value": "text/html",
              "type": "simple"
            },
            {
              "object": "s_bytes",
              "operation": ">",
              "value": "1000",
              "type": "simple"
            }
          ]
        }
      ]
    },
    "period": {
    }
  },
